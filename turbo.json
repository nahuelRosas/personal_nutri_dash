{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "**/.env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "**/.env*"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "**/.env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": false,
      "persistent": false
    },
    "clean": {
      "persistent": false
    },
    "docker:run": {
      "dependsOn": ["^docker:run"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "**/.env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": false,
      "persistent": false
    }
  }
}
